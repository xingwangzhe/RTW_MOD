{
  "name": "RustedWarfareIni",
  "scopeName": "source.ini",
  "fileTypes": ["ini", "txt"],
  "patterns": [
    {
      "begin": "^\\[(?<SectionName>[^\\]]+)\\]$",
      "beginCaptures": {
        "1": { "name": "entity.name.section.ini" }
      },
      "end": "^\\[",
      "endCaptures": {
        "0": { "name": "punctuation.section.ini" }
      },
      "patterns": [
        {
          "match": "^(?<key>[^:]+)(:)(?<value>.*)$",
          "captures": {
            "1": { "name": "keyword.other.definition.ini" },
            "2": { "name": "punctuation.separator.key-value.ini" },
            "3": { "name": "string.unquoted.value.ini" }
          }
        },
        {
          "include": "#comments"
        }
      ]
    },
    {
      "include": "#comments"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "match": "#.*$",
          "name": "comment.line.number-sign.ini"
        },
        {
          "match": ";.*$",
          "name": "comment.line.semicolon.ini"
        }
      ]
    }
  }
}